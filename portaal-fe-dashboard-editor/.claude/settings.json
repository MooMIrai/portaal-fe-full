{
  "project": {
    "name": "Intelligent Agent Selection Project",
    "description": "Project configured to use selector-agent for optimal task routing"
  },
  "behavior": {
    "proactiveAgentUse": true,
    "agentSelectionPolicy": "selector-first",
    "verbosity": "show-agent-selection"
  },
  "tools": {
    "task": {
      "defaultBehavior": "use-selector-agent",
      "showAgentReasoning": true
    }
  },
  "agents": {
    "selectorAgent": {
      "name": "selector-agent",
      "priority": "highest",
      "useForAmbiguousTasks": true
    },
    "availableAgents": [
      "incident-responder",
      "security-auditor",
      "debugger",
      "code-reviewer",
      "python-pro",
      "javascript-pro",
      "golang-pro",
      "rust-pro",
      "c-pro",
      "cpp-pro",
      "frontend-developer",
      "backend-architect",
      "cloud-architect",
      "deployment-engineer",
      "ml-engineer",
      "mlops-engineer",
      "ai-engineer",
      "data-scientist",
      "data-engineer",
      "database-admin",
      "database-optimizer",
      "sql-pro",
      "test-automator",
      "performance-engineer",
      "architect-reviewer",
      "api-documenter",
      "terraform-specialist",
      "network-engineer",
      "graphql-architect",
      "mobile-developer",
      "devops-troubleshooter",
      "payment-integration",
      "legacy-modernizer",
      "dx-optimizer",
      "error-detective",
      "search-specialist",
      "prompt-engineer",
      "quant-analyst",
      "risk-manager",
      "business-analyst",
