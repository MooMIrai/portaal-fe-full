  },
  "reminders": {
    "agentSelection": "Always use selector-agent first for technical tasks",
    "showProcess": "Display agent selection reasoning to user",
    "twoStepProcess": "1. Select agent with selector-agent, 2. Execute with selected agent"
  },
  "hooks": {
    "beforeTaskExecution": {
      "checkSelectorFirst": true,
      "message": "Consider using selector-agent for optimal agent selection"
    }
  }
}